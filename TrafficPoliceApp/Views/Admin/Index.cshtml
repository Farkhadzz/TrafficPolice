@model IEnumerable<User>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Age</td>
                <td>@user.Email</td>
                <td>
                    <a asp-action="AddFine" asp-controller="Admin" asp-route-userId="@user.Id">Add Fine</a>
                </td>
            </tr>
        }
    </tbody>
</table>
